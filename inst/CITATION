citHeader("To cite SBprofiles in publications use:")

citEntry(
  entry    = "Misc",
  title    = "SBprofiles",
  author   = personList(as.person("Paul R. Hibbing")),
  year     = "2021",
  url      = "https://github.com/paulhibbing/SBprofiles",
  textVersion = paste("Paul R. Hibbing (2021). SBprofiles.",
    "URL: https://github.com/paulhibbing/SBprofiles")
)
